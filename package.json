{
  "name": "naturalweb",
  "version": "1.0.0",
  "private": true,
  "description": "Natural language website interaction platform",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "type-check": "turbo run type-check"
  },
  "devDependencies": {
    "prettier": "^3.1.0",
    "tailwindcss-animate": "^1.0.7",
    "turbo": "^1.13.4",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@vitejs/plugin-react-swc": "^4.2.2",
    "selfsigned": "^5.2.0",
    "undici": "^7.16.0",
    "vite": "^7.2.7"
  }
}
